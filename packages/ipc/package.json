{
  "name": "@kiosk/ipc",
  "version": "1.0.0",
  "description": "IPC handlers and preload scripts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preload": {
      "types": "./dist/preload.d.ts",
      "default": "./dist/preload.js"
    },
    "./admin-preload": {
      "types": "./dist/admin-preload.d.ts",
      "default": "./dist/admin-preload.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kiosk/device": "workspace:*",
    "@kiosk/logger": "workspace:*",
    "@kiosk/platform": "workspace:*",
    "@kiosk/shared": "workspace:*"
  },
  "devDependencies": {
    "electron": "^40.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "license": "ISC"
}
